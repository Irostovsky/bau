<% content_tag :div, :class => "jqmWindow show_photo_dialog" do %>
	<%= link_to  'Закрыть', "#", :class => 'close_dialog' %>
	<div class="ad-gallery">
	  <div class="ad-image-wrapper">
	  </div>
	  <div class="ad-controls">
	  </div>
	  <div class="ad-nav">
	    <div class="ad-thumbs">
	      <ul class="ad-thumb-list">
					<% photos.each do |photo| %>
		        <li>
	            <%= link_to image_tag(photo.avatar.url(:medium)), photo.avatar.url(:big), :id => "photo_#{photo.id}" %>
		        </li>
					<% end -%>
	      </ul>
	    </div>
	  </div>
	</div>
<% end -%>
